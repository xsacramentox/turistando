<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imagemSPHome"
        android:layout_width="177dp"
        android:layout_height="94dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="210dp"
        android:layout_marginBottom="637dp"
        android:adjustViewBounds="true"
        android:scaleType="centerCrop"
        android:src="@color/colorAccent"
        android:translationX="-7dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:shapeAppearanceOverlay="@style/roundsquare" />

    <TextView
        android:id="@+id/textoTituloHome"
        style="@style/TextAppearance.AppCompat.Title"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="195dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="702dp"
        android:layout_toRightOf="@+id/image_rot"
        android:fontFamily="@font/poppins_semibold"
        android:text="Name"
        android:textColor="@color/black"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textoCidadeHome"
        style="@style/TextAppearance.AppCompat.SearchResult.Subtitle"
        android:layout_width="211dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="195dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="678dp"
        android:layout_toRightOf="@+id/image_rot"
        android:fontFamily="@font/poppins_semibold"
        android:maxLength="50"
        android:maxLines="2"
        android:singleLine="true"
        android:text="Des"
        android:textColor="@color/black"
        android:textSize="12dp"
        android:translationX="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textoDataHome"
        style="@style/TextAppearance.AppCompat.SearchResult.Subtitle"
        android:layout_width="211dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="196dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="654dp"
        android:layout_toRightOf="@+id/image_rot"
        android:fontFamily="@font/poppins_semibold"
        android:maxLength="50"
        android:maxLines="2"
        android:singleLine="true"
        android:text="Des"
        android:textColor="@color/black"
        android:textSize="12dp"
        android:translationX="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textoStatusHome"
        style="@style/TextAppearance.AppCompat.SearchResult.Subtitle"
        android:layout_width="211dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="195dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="627dp"
        android:layout_toRightOf="@+id/image_rot"
        android:fontFamily="@font/poppins_semibold"
        android:maxLength="50"
        android:maxLines="2"
        android:singleLine="true"
        android:text="Des"
        android:textColor="@color/black"
        android:textSize="12dp"
        android:translationX="25dp"
        android:translationY="-4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textoDescricaoHome"
        style="@style/TextAppearance.AppCompat.SearchResult.Subtitle"
        android:layout_width="394dp"
        android:layout_height="91dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="536dp"
        android:alpha=".5"
        android:fontFamily="@font/poppins_semibold"
        android:maxLength="100"
        android:maxLines="100"
        android:singleLine="false"
        android:text="Des"
        android:textAlignment="textStart"
        android:textSize="14dp"
        android:translationX="-5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>